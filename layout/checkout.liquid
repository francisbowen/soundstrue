  <body class="checkout-template">
    {{ skip_to_content_link }}

    <div class="banner" data-header>
      <div class="wrap">
        
      </div>
    </div>

    {{ order_summary_toggle }}

    <div class="content" data-content>
      <div class="wrap">
        <div class="main" role="main">
          <div class="main__header">
            <div class="logo-wrapper">
              {{ content_for_logo }}
            </div>
            <div class="checkout-progress-wrapper">
              {{ breadcrumb }}
            </div>
            {% if settings.display_exprss_checkout %}
            {{ alternative_payment_methods }}
            {% endif %}
          </div>
          <div class="main__content">
            <div class="go-daddy-badge">
              <span id="siteseal"><script async type="text/javascript" src="https://seal.godaddy.com/getSeal?sealID=qY57xmIYaaubpFAKlDUdcEL8CdeUAk7Zl8TTv4QqbkzervEYDVUHYGxT5qn3"></script></span>
