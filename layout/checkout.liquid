            </div>
            {{ content_for_layout }}
            <!-- should include password-after-checkout -->
  {% include 'password-after-checkout' %}
          </div>
          <div class="main__footer">
            {{ content_for_footer }}
          </div>
        </div>
        <div class="sidebar" role="complementary">
          <div class="sidebar__header">
            {{ content_for_logo }}
          </div>
          <div class="sidebar__content">
            {{ content_for_order_summary }}
          </div>
        </div>
      </div>
    </div>
    <!-- End Google Tag Manager (noscript) -->
    {{ tracking_code }}
    
    
    <style>
      .section--shipping-method del,
      .radio__label__accessory br {
        display:none;
      }
      .checkout-template .content .field__input-btn svg {
      	display:none !important;
      }
      .checkout-template .content .field__input-btn span.btn__content {
      	position:static !important;
      }      
      @media only screen and (max-width: 999px){
          .sidebar{	
              order: 1;
          }
      }
    </style>
    
  </body>
  <script>
    $('.edit_checkout').append('<input class="no-ready" type="hidden" name="checkout[attributes][guest_checkout]" value="{% if customer.id > 0 %}false{% else %}true{%endif%}" />');

    $(document).on(`page:load`, function() {
      if (Shopify.Checkout.step == 'contact_information'){
        document.getElementById('checkout_shipping_address_address1').setAttribute("autocomplete", "none");
        document.getElementById('checkout_shipping_address_address2').setAttribute("autocomplete", "none");
        document.getElementById('checkout_shipping_address_city').setAttribute("autocomplete", "none");
        document.getElementById('checkout_shipping_address_zip').setAttribute("autocomplete", "none");
      }
      //customer id: {{ customer.id  }}
      //$('.edit_checkout').append('<input type="hidden" name="checkout[attributes][guest_checkout]" value="{% if customer.id > 0 %}false{% else %}true{%endif%}" />');
    });
    $(document).ready(function() {
      var removeButtonInterval = setInterval(function() {
        if(jQuery('.remove-free-gift-card').length < 1) {
          jQuery('.reduction-code__text').each(function() {
            if(jQuery(this).text().includes('FREE GIFT CARD!')) {
              console.log('show remove link here'); 
              jQuery(this).parent().after('<li><a href="#" class="remove-free-gift-card">remove</a></li>');
            } 
          }); 
        }
      },100);
      jQuery('.product-table').on('click','.remove-free-gift-card',function(e) {
        console.log('clicked');
        e.preventDefault();
        var variant = parseInt(jQuery(this).closest('tr').data('variant-id'));
        console.log('variant id: '+variant);
        jQuery.ajax({
          url: '/cart/change.js',
          dataType: 'json',
          cache: false,
          type: 'post',
          data: {
            id: variant,
            quantity: 0
          },
          success: function (data) {
            console.log(JSON.stringify(data));
            //refreshCart(data);
            location.reload();
          }
        }).always(function(data) {
            console.log(JSON.stringify(data));
            location.reload();
        });
      });
      if($(window).width() < 999){
        jQuery('.sidebar').insertBefore('.main');
      }
    });
      $(document).ready(function(){
      	$('.product__description__name').wrap('<a href="#"></a>');
        //add floating label to discount code box
        $('body').on('focus','#checkout_reduction_code',function() {
          console.log('checkout_reduction_code focus');
          $(this).closest('.field__input-wrapper').addClass('field--show-floating-label').addClass('wick-discount-field');
        });
        $('body').on('blur','#checkout_reduction_code',function() {
          console.log('checkout_reduction_code blur');
          if($(this).val() == '') {
            $(this).closest('.field__input-wrapper').removeClass('field--show-floating-label').removeClass('wick-discount-field');
          }
        });
      })
  </script>
